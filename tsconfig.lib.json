{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/lib",
    "target": "es2022",
    "inlineSources": true,
    "types": [],
    "lib": [
      "dom",
      "es2022"
    ]
  },
  "angularCompilerOptions": {
    "compilationMode": "partial",
    "strictMetadataEmit": true,
    "enableResourceInlining": true
  },
  "exclude": [
    "src/test.ts",
    "**/*.spec.ts"
  ]
}
